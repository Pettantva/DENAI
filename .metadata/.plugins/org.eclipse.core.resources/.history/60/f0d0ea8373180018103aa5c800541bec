package DecisionTree;

import java.util.ArrayList;

public class Tree {
	private String name;
	//private String cause;
	private ArrayList<Tree> leafs;
	
	public Tree(String name){
		this.name = name;
		//this.cause = cause;
		this.leafs = new ArrayList<Tree>();
	}
	
	public void addChild(Tree child){
		leafs.add(child);
	}
	
}
